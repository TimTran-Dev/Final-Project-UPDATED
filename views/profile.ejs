	<body>

	<div class="gtco-loader"></div>

	<div id="page">


	<!-- <div class="page-inner"> -->
	<nav class="gtco-nav" role="navigation">
		<div class="gtco-container">

			<div class="row">
				<div class="col-sm-4 col-xs-12">
					<div id="gtco-logo"><a href="/">Rate My School <em>.</em></a></div>
				</div>
				<div class="col-xs-8 text-right menu-1">
					<ul>
						<li><a href="./">Sign Out</a></li>
						<li><a href="/">Home</a></li>
					</ul>
				</div>
			</div>

		</div>
	</nav>

	<header id="gtco-header" class="gtco-cover gtco-cover-sm" role="banner" style="background-image: url(images/img_3.jpg)">
		<div class="overlay"></div>
		<div class="gtco-container">
			<div class="row">
				<div class="col-md-12 col-md-offset-0 text-center">
					<div class="row row-mt-15em">

						<div class="col-md-12 mt-text animate-box" data-animate-effect="fadeInUp">
							<section class="form">
								<ul class="messages">
				          <% for(var i=0; i<userInfo.length; i++) {%>
				          <li class="message">
				            <span contenteditable="true"><%= userInfo[i].name %></span>
				            <span contenteditable="true"><%= userInfo[i].school %></span>
										<span class="hide"><%= userInfo[i]._id %></span>
				            <span><i class="fas fa-edit" aria-hidden="true"></i></span>
										<span class="edit" aria-hidden="true">Update</span>
				            <span class="delete" aria-hidden="true">Remove</span>
				          </li>
				          <% } %>
				        </ul>
								<img id="myImg" src="" alt="your image" height=200 width=100><br>
								<form action="/profile" method="POST">
									<input type="file" name="photo" value="">
									<input type="text" name="name" value="" placeholder="Your Name">
									<input type="text" name="school" value="" placeholder="Your School">
									<button type="submit" name="button">Submit</button>
								</form>
							</section>
						</div>

					</div>

				</div>
			</div>
		</div>
	</header>

	
